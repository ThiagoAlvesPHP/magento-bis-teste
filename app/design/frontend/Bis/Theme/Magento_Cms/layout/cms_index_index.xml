<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>

    </head>
    <body>
        <referenceContainer name="header.container">
            <block class="Magento\Theme\Block\Html\Header\Logo" name="header.top" template="Magento_Cms::html/header-top.phtml" />
        </referenceContainer>

        <referenceContainer name="page.top">
            <block class="Magento\Framework\View\Element\Template" name="pagetop.css.file" template="Magento_Cms::html/carousel.phtml" />
        </referenceContainer>
        <referenceContainer name="main">
            <block class="Magento\Framework\View\Element\Template" name="main.css.file" template="Magento_Cms::html/container.phtml" />
        </referenceContainer>
        <referenceContainer name="page.bottom.container">
            <block class="Magento\Framework\View\Element\Template" name="newsletter" template="Magento_Cms::html/newsletter.phtml" />
        </referenceContainer>
        <referenceContainer name="footer-container">
            <block class="Magento\Theme\Block\Html\Header\Logo" name="footer" template="Magento_Cms::html/footer.phtml" />
        </referenceContainer>
        <referenceContainer name="before.body.end">
            <block class="Magento\Framework\View\Element\Template" name="footer-end" template="Magento_Cms::html/footer-end.phtml" />
        </referenceContainer>
    </body>
</page>
